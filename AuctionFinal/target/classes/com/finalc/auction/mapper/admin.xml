<?xml version="1.0" encoding="UTF-8"?>

<!-- ==== mapper 기본설정 ==== -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<!-- ==== 루트 엘리먼트 & 네임스페이스 설정(프로젝트 전체내에서 유일해야 한다.) ==== -->
<mapper namespace="admin">
	<!-- 관리자 페이지 메인 단 -->
	<select id="getadMin" resultType="getadMinMap" parameterType="String">
		
	</select>
	
	<resultMap type="HashMap" id="getCategoryList">
		<result property="CNUM" column="cnum" javaType="String" />
		<result property="CNAME" column="cname" javaType="String" />
	</resultMap>
	
	<!-- 카테고리 관리 -->
	<select id="getCategoryList" resultType="com.finalc.auction.model.CategoryVO">
		select cnum, cname
		from tbl_category
	</select>
	
	<resultMap type="HashMap" id="getCategoryDetailList">
		<result property="CDNUM" column="cdnum" javaType="String" />
		<result property="FK_CNUM" column="fk_cnum" javaType="String" />
		<result property="CDNAME" column="cdname" javaType="String" />
	</resultMap>
	
	<select id="getCategoryDetailList" resultType="com.finalc.auction.model.CategoryVO">
		select cdnum, fk_cnum, cdname
		from tbl_category_detail
	</select>
	
	
</mapper>