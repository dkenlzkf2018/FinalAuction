<?xml version="1.0" encoding="UTF-8"?>

<!-- ==== mapper 기본설정 ==== -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<!-- ==== 루트 엘리먼트 & 네임스페이스 설정(프로젝트 전체내에서 유일해야 한다.) ==== -->
<mapper namespace="admin">
	<select id="getAdminCategory" resultType="com.finalc.auction.model.CategoryVO">
		select cnum, cname, cdnum, fk_cnum, cdname
		from tbl_category A join tbl_category_detail B on A.cnum = B.fk_cnum
		<!-- from tbl_category A join tbl_category_detail B on A.userid = B.fk_userid -->
	</select>
</mapper>